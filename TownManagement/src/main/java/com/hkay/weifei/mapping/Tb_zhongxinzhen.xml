<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.hkay.weifei.Dao.TownDao">
	<resultMap id="BaseResultMap" type="com.hkay.weifei.pojo.Tb_zhongxinzhen" />
	<insert id="inserttowninfo" parameterType="com.hkay.weifei.pojo.Tb_zhongxinzhen">
		insert into
		tb_zhongxinzhen(centertownname,number,attributionarea,townlevel,cooperation,townarea,builtuparea,townpopulation,floatepopulation,countygdp,countyrevenue,townrevenue,hundredcounties,citypilot,community,adminvillage,farmingoutvalue,partycommittee,committelnumber,industryoutvalue,mayor,mayortelnumber,serviceoutvalue,weather,terrain,traffic,currentindustry,industrialorientation,honorarytitle,historyculture,statuspic,planpic,docking)
		values
		(#{centertownname},#{number},#{attributionarea},#{townlevel},#{cooperation},#{townarea},#{builtuparea},#{townpopulation},#{floatepopulation},#{countygdp},#{countyrevenue},#{townrevenue},#{hundredcounties},#{citypilot},#{community},#{adminvillage},#{farmingoutvalue},#{partycommittee},#{committelnumber},#{industryoutvalue},#{mayor},#{mayortelnumber},#{serviceoutvalue},#{weather},#{terrain},#{traffic},#{currentindustry},#{industrialorientation},#{honorarytitle},#{historyculture},#{statuspic},#{planpic},#{docking})
	</insert>
	<select id="querytowninfo" parameterType="com.hkay.weifei.pojo.Tb_zhongxinzhen"
		resultMap="BaseResultMap">
		select t.* from tb_zhongxinzhen t where 1=1
	</select>
	<select id="querytowninfocnt" parameterType="com.hkay.weifei.pojo.Tb_zhongxinzhen"
		resultType="Integer">
		select count(*) from tb_zhongxinzhen t
		where 1=1
	</select>
	<select id="querytowndetail" parameterType="com.hkay.weifei.pojo.Tb_zhongxinzhen"
		resultMap="BaseResultMap">
		select t.* from tb_zhongxinzhen t where t.centertownid=#{centertownid}
	</select>
</mapper>